@model IEnumerable<StudentsCredits>

@{
    ViewData["Title"] = "StudentsCreditsReport";
}

<h2 class="set-top-margin">Hallgatók és szerzett kreditjeik</h2>

<table class="table">
    <thead>
        <tr>
            <th>
                Vezetéknév
            </th>
            <th>
                Keresztnév
            </th>
            <th>
                Szerzett Kredit
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.LastName
                </td>
                <td>
                    @item.FirstMidName
                </td>
                <td>
                    @item.Credits
                </td>
            </tr>
        }
    </tbody>
</table>
